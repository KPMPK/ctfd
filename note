sudo docker build -f Dockerfile.frontend -t ctfd-frontend .
sudo docker run -d --name ctfd-frontend   --network ctf-net   -p 8080:8080  --user 0  ctfd-frontend
sudo docker build -f Dockerfile.backend -t ctfd-backend .
sudo docker run -d --name ctfd-backend   --network ctf-net   --network-alias ctf.ai.local   -p 3001:3001   -v $(pwd)/data:/data   ctfd-backend





 sudo docker run -d --name level1   --network ctf-net   -p 8501:8501 level1  
